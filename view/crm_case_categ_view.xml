<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

    <!--  CRM case categ inherited form -->
    <!-- -->
    <record id="crm_case_categ-view" model="ir.ui.view">
        <field name="name">enhanced_helpdesk_crm.crm_case_categ</field>
        <field name="model">crm.case.categ</field>
        <field name="inherit_id" ref="crm.crm_case_categ-view"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="emergency" />
            </field>
        </field>
   </record>
    
    
    <!-- CRM case categ inherited tree -->
    <record id="crm_case_categ_tree-view" model="ir.ui.view">
        <field name="name">enhanced_helpdesk_crm.crm_case_categ_tree</field>
        <field name="model">crm.case.categ</field>
        <field name="inherit_id" ref="crm.crm_case_categ_tree-view"/>

        <field name="arch" type="xml">

           <field name="name" position="after">
                <field name="emergency" />
            </field>

        </field>
    </record>

</data>
</openerp>
